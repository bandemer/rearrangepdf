{% extends 'bootstrap4.html.twig' %}

{% block body %}

<div class="container">
	
	<div class="jumbotron">            
	    <h1>Upload PDF-File</h1>
	        
	    {% if app.session.flashBag.has('error') %}
	    <div class="alert alert-danger" role="alert">
	        {% for msg in app.session.flashBag.get('error') %}
	            {{ msg }}
	        {% endfor %}
	    </div>
	    {% endif %}
	    
	    {{ form_start(form) }}
	    {{ form_widget(form) }}
	    {{ form_end(form) }}
    </div>
        
</div>
{% endblock %}


